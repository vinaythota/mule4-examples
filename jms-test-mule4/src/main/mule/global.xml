	<configuration-properties file="test.yaml" />
	
		<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="92f547d6-cb82-47af-af72-b819ece4e96a" >
		<http:listener-connection host="0.0.0.0" port="${http.port}" />
